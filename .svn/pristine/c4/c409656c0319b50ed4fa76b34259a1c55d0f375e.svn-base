html,body{
    height:100%;
    margin: auto;
    padding:0;
}
ol,ul,dl,li,dt,dd {
	list-style:none;
}
#left_main{
    float: left;
    margin-left: 5px;
    margin-top: 15px;
    padding-bottom: 1em;
    border-left: 1px solid #CCC;
    border-right: 1px solid #CCC;
    border-bottom: 1px solid #CCC;
    min-height: 450px;
    height: auto;
    width: 190px;
    background-color: #fff;
}
#navigation {
    height: 100%;
    margin:0px auto;
    padding:0px;
    width:188px;
}
#navigation a.head {
    cursor:pointer;
    background:url(/assets/main_34.gif) no-repeat scroll;
    display:block;
    font-weight:bold;
    margin:0px;
    padding:5px 0 5px;
    text-align:center;
    font-size:12px;
    text-decoration:none;
}
#navigation ul {
    border-width:0px;
    margin:0px;
    padding:0px;
    text-indent:0px;
}
#navigation li {
    list-style:none;
    /*        display:inline;*/
}
#navigation li li a {
    display:block;
    font-size:12px;
    text-decoration: none;
    text-align:center;
    padding:3px;
}
#navigation li li a:hover, .current_select {
    background:url(/assets/tab_bg.gif) repeat-x;
    border:solid 1px #adb9c2;
}
#center{
    margin-top: 35px;
    margin-left: 11em;
    padding-left: 3em;
    min-height: 580px;
    height:auto;
}
#reg{
    margin-left: 1em;
    padding-left: 1em;
}

#mainapp .entry{
    display: inline;
}


#form1 table {
    color: #000000;
    font-size:14px;
    width: 400px;
    border: 1px solid #636363;
}
#form1 th {
    font-size: 14px;
    color: #ffffff;
    background-color: #636363;
}
