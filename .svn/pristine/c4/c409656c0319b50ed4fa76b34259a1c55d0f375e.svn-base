#search {
    font-size: 16px;
    float: left;
    line-height:25px;
    text-align: center;
    width:100%;
    margin: 10px 30px;
}

#highLevelSearch {
    font-size: 12px;
    text-align: left;
    float: left;
    width:100%;
}
#highLevelSearch ul{
    float: left;
    text-align: left;
    width:100%;
    padding: 10px;
}
#highLevelSearch li {
    float:left;
    line-height:30px;
    list-style:none;
    width:100%;
}

#highLevelSearch li .l {
    float:left;
    width: 35%;
    text-align: right;
}
#highLevelSearch li .r {
    float:right;
    width: 60%;
    text-align: left;
}
.validate_div{
    color: red;
}

.search_border {
    border-color: #818181 #B8B8B8 #B8B8B8 #818181;
    border-right: none;
    float: left;
}

.search_query {
    width: 481px;
    padding: 4px;
    height: 21px;
    border: 1px solid #D6D6D6;
    border-width: 1px 0 0 1px;
    float: left;
    font: 16px/21px arial;
    margin: 0;
    color: black;
    vertical-align: top;
    background-color: white;
}

#search_button {
    float: left;
    width: 90px;
    height: 33px;
    background: #D4D4D4 url(http://www.sogou.com/images/ns.png) no-repeat left -340px;
    border: 0;
    margin: 0;
    padding: 0;
    cursor: pointer;
    font: 14px simsun;
    color: black;
}

#search_uri {
    font-size: 12px;
    float: left;
    margin-left: 10px;
}

#search_time_select{
    float:left;
}
.search_border span{
    font-size: 12px;
    float:left;
}

