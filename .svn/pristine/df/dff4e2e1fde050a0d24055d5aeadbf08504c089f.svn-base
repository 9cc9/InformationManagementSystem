/*student list format*/
html {width:100%; height:100%;}
body {
    position: relative;
    height: 100%;
    min-width:  720px;
    min-height: 550px;
    margin: 0;
    /*background-color: #6495ed;*/
    /*background:url("login00.GIF") repeat-x #fff8dc;*/
}
body, table, select, input, button {
    font-size: 12px;
    font-family: tahoma, arial, sans-serif;
    background-color: #dce0ff;
}
input {
    border: 1px solid #7F9DB9;
    padding:5px;
}
.fLeft {float: left;}
.fRight {float: right;}
a:link, a:visited, a:active {
    color: #1059A0;
}
a:hover {
    color: #10A5DD;
}
.Head {
    margin: 10px auto;
    height: 40px;
    width: 100%;
    /*background: #05156a;*/
    /*background: url(logo05.GIF) repeat-x;#327fca*/
}
.HeadLeft {
    width: 25%;
    height: 100px;
    /*background-color: #6495ed ;*/
}
.HeadRight{
    width:75%;
    height: 100px;
    /*background: url(logo5.jpg) #6495ed no-repeat ;*/
}
.Head a:link, .Head a:visited, .Head a:active  {
    color: #FFFFFF;
    text-decoration: none;
}
.Head a:hover {
    text-decoration: underline;
}
a.HeLink:link, a.HeLink:visited, a.HeLink:active {
    color: #FFF000;
    text-decoration: none;
}
a.HeLink:hover {
    text-decoration: underline;
}
.MainLeft {
    float: left;
    height: 680px;
    width: 50%;
    margin-top:80px;
    text-align: right;
    padding-right: 30px;
    border-right: 1px solid #DADADA;
    background: url(login.jpg) no-repeat;
}
.MainLeft .slogan1,.MainLeft .slogan2,.MainLeft .slogan3 {
    margin-top: 20px;
    line-height: 18px;
}
.MainLeft .slogan1{
    color: #10A5DD;
}
.MainLeft .slogan2 {
    color: #6D6D6D;
}
.MainLeft .slogan3 a{
    font-family:sans-serif,serif;
}
.MainRight {
    float: right;
    height: 320px;
    width: 45%;
    margin-top: 120px;
    color:  #626262;
    /*#626262;*/
}
.title .caption {
    font-size: 14px;
    font-weight: bold;
    color: #060303;
    padding-top: 10px;
    padding-bottom: 20px;
    text-align: left;
    white-space: nowrap;
}
.title .titleNote {
    font-size: 12px;
    font-weight: normal;
    padding-left: 30px;
}
.loginTable td, .loginTable th {
    padding: 8px;
    padding-right: 0;
    color:#060501;
}
.loginTable th {
    font-weight: normal;
    text-align: right;
    width:57px;
}
.loginTable .pwdTh {
    width:90px;
}
