body {
	margin:0;
	padding:0;
	overflow:hidden;
	font-size: 12px;
	color: #ff0000;
}
#login {
	margin: 0 auto;
	clear:both;
        width: 100%;
        height: 900px;
	background:url(login_03.png) repeat-y;
}
#login .logo {
        width: 100%;
        height: 160px;
/*        padding-left: 300px;*/
}

#center {
	height:84px;
	text-align:center;
}
#center_left {
	margin-left:216px;
	float:left;
	background:url(login_06.jpg) no-repeat;
	height:84px;
	width:381px;
}
#error_login{
    color: #ff0000;
    font-size: 15px;
}
/* IE7 HACK*/
*+html #center_left {
	margin-left:206px !important;
}
/* END HACK*/
#center_middle {
/*	float:left;*/
/*	background:url(/assets/login_07.jpg) no-repeat;*/
        margin-top: 200px;
	height:auto;
	width:100%;
}
#center_submit {
	float:left;
	height:30px;
	width:100%;
}
#footer {
	margin:0 auto;
	height:206px;
	width:847px;
}
INPUT {
	width:100px;
	height:17px;
/*	background-color:#87adbf;*/
	border:solid 1px #153966;
	font-size:12px;
/*            #283439;*/
}
.chknumber_input {
	width:40px;
}
.user {
	margin: 6px auto;
        height: 30px;
        font-size: 14px;
        font-weight: bold;
}
/* IE7 HACK*/
*+html .user {
	margin: 4px auto;
}
/* END HACK*/
.chknumber {
	margin-bottom:3px;
	text-align:left;
	padding-left:3px
}
IMG {
	border:none;
	cursor:pointer;
}
FORM {
	margin:0;
	padding:0
}
